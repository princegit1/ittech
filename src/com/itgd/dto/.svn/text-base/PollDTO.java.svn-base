package com.itgd.dto;

public class PollDTO {
	private int pollId;
	private String pollTitle;
	private int pollOptionId;
	private String pollOptionTitle;
	private int pollOptionHits;
	private String pollDateFirst;
	private String pollDateLast;
	private String previousPollIds;
	
	public int getPollId() {
		return pollId;
	}
	public void setPollId(int pollId) {
		this.pollId = pollId;
	}
	public String getPollTitle() {
		return pollTitle;
	}
	public void setPollTitle(String pollTitle) {
		this.pollTitle = pollTitle;
	}
	public int getPollOptionId() {
		return pollOptionId;
	}
	public void setPollOptionId(int pollOptionId) {
		this.pollOptionId = pollOptionId;
	}
	public String getPollOptionTitle() {
		return pollOptionTitle;
	}
	public void setPollOptionTitle(String pollOptionTitle) {
		this.pollOptionTitle = pollOptionTitle;
	}
	public int getPollOptionHits() {
		return pollOptionHits;
	}
	public void setPollOptionHits(int pollOptionHits) {
		this.pollOptionHits = pollOptionHits;
	}
	public String getPollDateFirst() {
		return pollDateFirst;
	}
	public void setPollDateFirst(String pollDateFirst) {
		this.pollDateFirst = pollDateFirst;
	}
	public String getPollDateLast() {
		return pollDateLast;
	}
	public void setPollDateLast(String pollDateLast) {
		this.pollDateLast = pollDateLast;
	}
	public String getPreviousPollIds() {
		return previousPollIds;
	}
	public void setPreviousPollIds(String previousPollIds) {
		this.previousPollIds = previousPollIds;
	}	
}
