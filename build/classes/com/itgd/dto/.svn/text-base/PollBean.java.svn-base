package com.itgd.dto;

import java.io.Serializable;

public class PollBean implements Serializable{
	private int pollid;
	private int optionid;
	private String question;
	private String option;
	private String previousQuestion;
	private String previousOption;
	private int previousHits;
	private String polldates;
	private int previouspollid;
	
	public int getPreviouspollid() {
		return previouspollid;
	}
	public void setPreviouspollid(int previouspollid) {
		this.previouspollid = previouspollid;
	}
	public String getPolldates() {
		return polldates;
	}
	public void setPolldates(String polldates) {
		this.polldates = polldates;
	}
	public String getPreviousQuestion() {
		return previousQuestion;
	}
	public void setPreviousQuestion(String previousQuestion) {
		this.previousQuestion = previousQuestion;
	}
	public String getPreviousOption() {
		return previousOption;
	}
	public void setPreviousOption(String previousOption) {
		this.previousOption = previousOption;
	}
	public int getPreviousHits() {
		return previousHits;
	}
	public void setPreviousHits(int previousHits) {
		this.previousHits = previousHits;
	}
	public int getPollid() {
		return pollid;
	}
	public void setPollid(int pollid) {
		this.pollid = pollid;
	}
	public int getOptionid() {
		return optionid;
	}
	public void setOptionid(int optionid) {
		this.optionid = optionid;
	}
	public String getQuestion() {
		return question;
	}
	public void setQuestion(String question) {
		this.question = question;
	}
	public String getOption() {
		return option;
	}
	public void setOption(String option) {
		this.option = option;
	}

}
