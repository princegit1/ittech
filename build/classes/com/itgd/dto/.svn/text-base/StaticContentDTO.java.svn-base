package com.itgd.dto;

public class StaticContentDTO {
	private String staticContentTitle;
	private String staticContentText;
	private int featured;
	public String getStaticContentTitle() {
		return staticContentTitle;
	}
	public void setStaticContentTitle(String staticContentTitle) {
		this.staticContentTitle = staticContentTitle;
	}
	public String getStaticContentText() {
		return staticContentText;
	}
	public void setStaticContentText(String staticContentText) {
		this.staticContentText = staticContentText;
	}
	public int getFeatured() {
		return featured;
	}
	public void setFeatured(int featured) {
		this.featured = featured;
	}
	

}
